<script type="text/javascript">
    ltk.dom.ready(function() {
        ltk.l10n.setLocale("es_ES");
        ltk.l10n.setCurrency("EUR");

        var variants = [{"variant_id":16,"variant_name":"MP3 Musik","variant_description":"","variant_price":"13.90","variant_tag":"MP3_MUSIC","variant_class":4,"variant_premium":0,"variant_subscription":0,"variant_credits":0,"variant_price_brutto":"14.60","media_id":2290642,"variant_size":0,"variant_info":"","group_name":"","group_tag":"mp3","file_size":5442244,"socialmedialicense":"1"},{"variant_id":18,"variant_name":"Original Lossless","variant_description":"","variant_price":"19.90","variant_tag":"ORIGINAL_MUSIC","variant_class":4,"variant_premium":0,"variant_subscription":0,"variant_credits":0,"variant_price_brutto":"20.90","media_id":2290642,"variant_size":0,"variant_info":"","group_name":"","group_tag":"mp3","file_size":26118946,"socialmedialicense":"1"},{"variant_id":108,"variant_name":"Original Lossless","variant_description":"","variant_price":"59.90","variant_tag":"MERCH_ORIGINAL_MUSIC","variant_class":4,"variant_premium":0,"variant_subscription":0,"variant_credits":0,"variant_price_brutto":"62.90","media_id":2290642,"variant_size":0,"variant_info":"","group_name":"Merchandising Lizenz","group_tag":"original","file_size":26118946,"socialmedialicense":"1"}];

        var vat = parseInt("5", 10) || 0;
        var socialLicensePrice = parseFloat("1.5");
        var socialLicenseBtn = ltk.dom.one('.socialLicenseBtn');
        var exchange = parseFloat(1) || 1;

        if (socialLicenseBtn) {
            (function() {
                socialLicenseBtn.node.checked = 'checked';

                variants.forEach(function(variant) {
                    if(vpn = ltk.dom.one('.variant_price_netto_'+variant.variant_id)) {
                        var sociallicense = ltk.dom.one('.sociallicense_'+variant.variant_id);
                        var currentPrice = parseFloat(variant.variant_price);
                        vpn.node.innerHTML = ltk.l10n.monf((currentPrice+socialLicensePrice) * exchange);
                        sociallicense.node.value = 1;
                    }

                    if(vpb = ltk.dom.one('.variant_price_brutto_'+variant.variant_id))
                        vpb.node.innerHTML = ltk.l10n.monf( ((currentPrice+socialLicensePrice) + ((currentPrice+socialLicensePrice) * (vat / 100))) * exchange);
                });
            })();

            ltk.evt.addEvent(socialLicenseBtn, 'change', function() {
                if(socialLicenseBtn.node.checked) {
                    variants.forEach(function(variant) {
                        if(vpn = ltk.dom.one('.variant_price_netto_'+variant.variant_id)) {
                            var sociallicense = ltk.dom.one('.sociallicense_'+variant.variant_id);
                            var currentPrice = parseFloat(variant.variant_price);
                            vpn.node.innerHTML = ltk.l10n.monf((currentPrice+socialLicensePrice) * exchange);
                            sociallicense.node.value = 1;
                        }

                        if(vpb = ltk.dom.one('.variant_price_brutto_'+variant.variant_id))
                            vpb.node.innerHTML = ltk.l10n.monf( ((currentPrice+socialLicensePrice) + ((currentPrice+socialLicensePrice) * (vat / 100))) * exchange);
                    });
                } else {
                    variants.forEach(function(variant) {
                        if(vpn = ltk.dom.one('.variant_price_netto_'+variant.variant_id)) {
                            var sociallicense = ltk.dom.one('.sociallicense_'+variant.variant_id);
                            var currentPrice = parseFloat(variant.variant_price);
                            vpn.node.innerHTML = ltk.l10n.monf(currentPrice * exchange);
                            sociallicense.node.value = 0;
                        }

                        if(vpb = ltk.dom.one('.variant_price_brutto_'+variant.variant_id))
                            vpb.node.innerHTML = ltk.l10n.monf( (currentPrice + (currentPrice * (vat / 100))) * exchange);
                    });
                }
            });
        }

        // subscription
        var socialLicenseCredits = parseInt("1");
        var socialLicenseAboBtn = ltk.dom.one('.socialLicenseAboBtn');
        var variant_credit = ltk.dom.one('.variant_credit');

        if (socialLicenseAboBtn) {
            (function() {
                socialLicenseAboBtn.node.checked = 'checked';

                variants.forEach(function(variant) {
                    var sociallicense = ltk.dom.one('.sociallicense_'+variant.variant_id);

                    if(variant.group_tag == 'subscription') {
                        variant_credit.node.innerHTML = (parseInt(variant_credit.node.innerHTML)+socialLicenseCredits);
                        sociallicense.node.value = 1;
                    }
                });
            })();

            ltk.evt.addEvent(socialLicenseAboBtn, 'change', function() {
                if(socialLicenseAboBtn.node.checked) {
                    variants.forEach(function(variant) {
                        var sociallicense = ltk.dom.one('.sociallicense_'+variant.variant_id);

                        if(variant.group_tag == 'subscription') {
                            variant_credit.node.innerHTML = (parseInt(variant_credit.node.innerHTML)+socialLicenseCredits);
                            sociallicense.node.value = 1;
                        }
                    });
                } else {
                    variants.forEach(function(variant) {
                        var sociallicense = ltk.dom.one('.sociallicense_'+variant.variant_id);

                        if(variant.group_tag == 'subscription') {
                            variant_credit.node.innerHTML = variant.variant_credits;
                            sociallicense.node.value = 0;
                        }
                    });
                }
            });
        }
    });
