clipdealer.detailspage.setMediaId(2290642);
  
function tabs(id) {
    this.tabObj = document.getElementById(id);
    this.active = 0;

    // attach events to tabs
    this.tabs = this.tabObj.getElementsByTagName('li');
    
    var me = this;
    
    function _newClosure(param, cb) {
        return function() {
            cb(param);
        }
    }
    
    for (var i = 0, len = this.tabs.length; i < len; ++i) {
        if (this.tabs[i].className.indexOf('activeTab') >= 0) {
            this.active = i;
        }
        
        this.tabs[i].onclick = _newClosure(i, function(no) {
            me.activateTab(no)
        });
    }

    // remember panes
    var tmp = this.tabObj.getElementsByTagName('div');
    this.panes = [];
     
    for (i = 0, len = tmp.length; i < len; ++i) {
        if (tmp[i].className.indexOf('tabContent') >= 0) {
            this.panes[this.panes.length] = tmp[i];
        }
    }
}
tabs.prototype.activateTab = function(no) {
    if (no == this.active) return;
    
    this.tabs[this.active].className = '';
    this.panes[this.active].className = 'tabContent';
    
    this.active = no;
    this.tabs[this.active].className = 'activeTab';
    this.panes[this.active].className += ' activeTab';
}

lima.addLoadEvent(function() {
    // setup player and tabs
    clipdealer.prevplayer.setup('https://preview.clipdealer.com/', 'https://buidln.clipdealer.com/');

    new tabs('box1');

    // setup voting tool
    var voting = new lima.voting('voting_stars', 'voting_indicator', '#7badd5', '#a9ca5f');
    voting.setValue(10);
    voting.deactivate();
    
    });// -- end lima.addLoadEvent --

    
